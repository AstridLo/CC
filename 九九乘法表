#include <iostream>
#include <string>
#include <stdio.h>
using namespace std;

int main() {
 printf("九九乘法表\n");
 int i = 0;
    int j = 0;
    int k = 0;
    char s[64];
    char t[64];
    char u[64];
    for (i = 1; i < 10; i++) {
    for (j = 1; j < 10; j++) {
    k = i * j ;

    sprintf( s, "%d", k );
    sprintf( t, "%d", i );
    sprintf( u, "%d", j );
    printf(t); 
    printf("*");
    printf(u);
    printf("=");
    printf(s);
    printf("\n");
    }   
    }
    ;
 
 
 return 0;
}
